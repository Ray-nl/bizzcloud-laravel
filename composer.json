{
    "name": "raynl/bizzcloud-laravel",
    "description": "Bizzcloud for Laravel",
    "homepage": "https://github.com/Ray-nl/bizzcloud-laravel",
    "keywords": ["Laravel","Bizzcloud","Statamic"],
    "autoload": {
        "psr-4": {
            "Raynl\\Bizzcloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raynl\\Bizzcloud\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Rolin Bos",
            "email": "rolin@ray.nl",
            "homepage": "https://github.com/rolinbos",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "rolin@ray.nl"
    },
    "extra": {
        "statamic": {
            "name": "Bizzcloud",
            "description": "Bizzcloud addon"
        },
        "laravel": {
            "providers": [
                "Raynl\\Bizzcloud\\BizzcloudServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "require": {
        "php": ">= 7.*",
        "darkaonline/ripcord": "^0.1.8"
    }
}
